<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - noter</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
        <link href="~/lib/users/font-awesome.css" rel="stylesheet" />
        <link href="~/lib/users/sb-admin-2.css" rel="stylesheet" />
        <link href="~/lib/users/custom.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/css/site.css"/>
    </environment>
    <environment exclude="Developent">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
        <link rel="stylesheet" href="~/css/site.css"/>
    </environment>
</head>
<body>

@{
    ViewBag.Title = "title";
}
@await Html.PartialAsync("_NavBar")
<nav class="navbar navbar-default" role="navigation" style="margin-bottom: 0">
    <div class="navbar-header">
        <a class="navbar-brand"  href="/">My Users</a>
    </div>
    <div class="navbar-default sidebar" role="navigation">
        @await Component.InvokeAsync("UserList")
    </div>
</nav>
<div id="page-wrapper">
    @RenderBody()
</div>
<environment include="Development">
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</environment>
<environment exclude="Development">
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</environment>
@RenderSection("Scripts", required: false)
</body>
</html>